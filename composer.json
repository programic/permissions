{
    "name": "programic/permissions",
    "license": "proprietary",
    "type": "package",
    "description": "Target specific roles & permissions",
    "keywords": [
        "programic",
        "laravel",
        "permission",
        "permissions",
        "roles",
        "acl",
        "rbac",
        "security"
    ],
    "readme": "README.md",
    "authors": [
        {
            "name": "Programic",
            "email": "development@programic.com",
            "homepage": "https://programic.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2.5|^8.0",
        "illuminate/auth": "^6.0|^7.0|^8.0",
        "illuminate/container": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Programic\\Permission\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Programic\\Permission\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Programic\\Permission\\PermissionServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
